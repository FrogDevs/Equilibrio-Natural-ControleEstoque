    "electron-window-state": "~5.0.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.3",
    "electron": "^19.0.0",
    "electron-builder": "~22.14.13",
    "electron-rebuild": "^3.2.7",
    "typescript": "~4.3.5"
  },
  "keywords": [
    "capacitor",
    "electron"
  ]
